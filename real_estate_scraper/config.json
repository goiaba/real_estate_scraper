{
  "conf": {
    "logging": {
      "config_file_path": "logging.ini"
    },
    "result_filename_date_format": "%Y-%m-%d_%H:%M:%S",
    "view_date_format": "%H:%M:%S (%d-%m-%Y)",
    "max_workers": 8,
    "data_directory": "data",
    "agencies": {
      "config_directory": "agencies_config",
      "files": ["cacapava-sp.json","americana_sbo-sp.json"]
    },
    "email": {
      "enabled": true,
      "subject": "Relatório de atualização de imóveis",
      "from_email": "",
      "to_emails": [],
      "template_name": "atualizacao_de_imoveis.html"
    },
    "search_filter": {
      "min_price": 2000,
      "max_price": 3500
    }
  },
  "headers": {
    "User-Agent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/79.0.3945.117 Safari/537.36"
  }
}
